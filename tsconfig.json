{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@angular-nrwl-demo/user": [
        "libs/user/src/index.ts"
      ],
      "@angular-nrwl-demo/shared-ui": [
        "libs/shared-ui/src/index.ts"
      ],
      "@env/*": [
        "apps/adminapp/src/environments/*"
      ],
      "@angular-nrwl-demo/notification": [
        "libs/notification/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
